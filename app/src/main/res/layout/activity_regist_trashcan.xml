<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DCFAB9"
    tools:context=".RegistTrashcan">

    <Button
        android:id="@+id/registTrashcanHomeButton"
        android:layout_width="81dp"
        android:layout_height="77dp"
        android:background="@drawable/listhomepagebtn2"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/registTrashcanAccountButton"
        android:layout_width="82dp"
        android:layout_height="70dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="248dp"
        android:background="@drawable/listuseraccountbtn"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/registTrashcanHomeButton"
        app:layout_constraintTop_toTopOf="@+id/registTrashcanLocationButton"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/registTrashcanLocationButton"
        android:layout_width="83dp"
        android:layout_height="70dp"
        android:background="@drawable/listlocationbtn"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/registTrashcanAccountButton"
        app:layout_constraintTop_toTopOf="@+id/registTrashcanMoneyButton" />

    <Button
        android:id="@+id/registTrashcanMoneyButton"
        android:layout_width="78dp"
        android:layout_height="70dp"
        android:background="@drawable/listmoneybtn"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/registTrashcanLocationButton"
        app:layout_constraintTop_toTopOf="@+id/registTrashcanSettingButton" />

    <Button
        android:id="@+id/registTrashcanSettingButton"
        android:layout_width="83dp"
        android:layout_height="70dp"
        android:background="@drawable/listsettingbtn"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/registTrashcanMoneyButton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/plasticbagCanStorage"
        android:layout_width="89dp"
        android:layout_height="58dp"
        android:text="10%"
        android:textColor="#F60000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.776"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.67" />

    <ImageView
        android:id="@+id/bottleCan"
        android:layout_width="118dp"
        android:layout_height="198dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.082"
        app:srcCompat="@drawable/can_plastic" />

    <ImageView
        android:id="@+id/plasticbagCan"
        android:layout_width="116dp"
        android:layout_height="199dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.813"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.558"
        app:srcCompat="@drawable/can_plasticbag" />

    <ImageView
        android:id="@+id/ironbottleCan"
        android:layout_width="119dp"
        android:layout_height="198dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.202"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.557"
        app:srcCompat="@drawable/can_ironbottel" />

    <TextView
        android:id="@+id/bottleCanStorage"
        android:layout_width="89dp"
        android:layout_height="58dp"
        android:text="10%"
        android:textColor="#F60000"
        android:textSize="48sp"
        app:layout_constraintBottom_toTopOf="@+id/ironbottleCan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.845" />

    <TextView
        android:id="@+id/IronbottleCanStorage"
        android:layout_width="89dp"
        android:layout_height="58dp"
        android:text="10%"
        android:textColor="#F60000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.251"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.67" />

    <Button
        android:id="@+id/button"
        android:layout_width="170dp"
        android:layout_height="51dp"
        android:background="@drawable/updatebtn"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toTopOf="@+id/registTrashcanLocationButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IronbottleCanStorage" />

</androidx.constraintlayout.widget.ConstraintLayout>